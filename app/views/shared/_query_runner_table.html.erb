<% if query_runner.empty? %>
  <div class="alert">
    <button type="button" class="close" data-dismiss="alert">&times;</button>
    <strong>No Results!</strong>
  </div>
<% else %>
  <table class="table table-stripped">
    <thead>
      <tr>
        <% query_runner.headers.each do |header| %>
          <th><%= header %></th>
        <% end %>
      </tr>
    </thead>
    <tbody>
      <% query_runner.results.each do |result| %>
        <tr>
          <% result.values.each do |value| %>
            <td><%= value %></td>
          <% end %>
        </tr>
      <% end %>
    </tbody>
  </table>
<% end %>
